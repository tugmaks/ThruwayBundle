{
  "name": "voryx/thruway-bundle",
  "type": "symfony-bundle",
  "description": "WebSockets (WAMP2) integration for Symfony2",
  "keywords": [
    "WebSockets",
    "WAMP",
    "WAMP2",
    "Autobahn",
    "Sockets",
    "Symfony"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "David Dan",
      "email": "davidwdan@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Matt Bonneau",
      "email": "matt@bonneau.net",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-0": {
      "Voryx\\ThruwayBundle": "src"
    }
  },
  "require": {
    "php": "^7.1",
    "doctrine/annotations": "^1.10",
    "react/child-process": "~0.4",
    "symfony/console": "^4.4|^5.0",
    "symfony/dependency-injection": "^4.4|^5.0",
    "symfony/event-dispatcher": "^4.4|^5.0",
    "symfony/event-dispatcher-contracts": "^1.1|^2.1",
    "symfony/finder": "^4.4|^5.0",
    "symfony/http-kernel": "^4.4|^5.0",
    "symfony/security-core": "^4.4|^5.0",
    "symfony/serializer": "^4.4|^5.0",
    "thruway/pawl-transport": "^0.5.0",
    "voryx/thruway": "~0.6"
  },
  "require-dev": {
    "doctrine/orm": "^2.7",
    "phpstan/phpstan": "^0.12.35",
    "phpunit/phpunit": "^9.2",
    "symfony/framework-bundle": "^4.4|^5.0",
    "symfony/property-access": "^4.4|^5.0",
    "symfony/yaml": "^4.4|^5.0"
  },
  "config": {
    "sort-packages": true
  }
}
